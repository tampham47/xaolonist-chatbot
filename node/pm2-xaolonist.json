{
	"apps": [
		{
			"exec_mode": "fork_mode",
			"name": "xaolonist-chatbot",
			"script": "app.js",
			"watch": false,
			"ignore_watch": [
				"node_modules",
				"log"
			],
			"watch_options": {
				"followSymlinks": false
			},
			"env": {
				"NODE_ENV": "development",
				"LOG": 1,
				"PORT": 5004
			},
			"env_production": {
				"NODE_ENV": "production",
				"PORT": 5004
			},
			"env_staging": {
				"NODE_ENV": "staging",
				"PORT": 5004
			}
		}
	]
}
